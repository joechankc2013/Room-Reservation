<table class="table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>User</th>
      <th>Room</th>
      <th>Start Time </th>
      <th>End Time</th>
      <th>Reserved By</th>
      <th>Updated At</th>
      <th>(Actor)</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% AdminReservationsDecorator.new(reservations).each do |reservation| %>
      <tr>
        <td><%= reservation.id %></td>
        <td><%= reservation.user_onid %></td>
        <td><%= reservation.room.name %></td>
        <td><%= reservation.formatted_start %></td>
        <td><%= raw reservation.formatted_end %></td>
        <td><%= reservation.reserver_onid %></td>
        <td><%= reservation.formatted_updated_at %></td>
        <td><%= reservation.current_originator %></td>
        <td><%= reservation.status_string %></td>
      </tr>
    <% end %>
  </tbody>
</table>
